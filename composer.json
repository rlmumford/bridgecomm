{
  "name": "rlmumford/bridgecomm",
  "type": "library",
  "description": "A library for interacting with BridgePay's bridgecomm",
  "authors": [
    {
      "name": "Rob Mumford",
      "email": "rlmumford@googlemail.com"
    }
  ],
  "require": {
    "php": "^7.2.5",
    "guzzlehttp/guzzle": "^6.3|^7.0",
    "ext-dom": "*",
    "ext-simplexml": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3@dev",
    "vlucas/phpdotenv": "^5.1@dev"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "BridgeComm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BridgeComm\\Tests\\": "tests/"
    }
  }
}
